    "x = 0\n",
    "y = 1\n",
    "sum = 0\n",
    "i = 0\n",
    "\n",
    "def fib(lim) -> int:\n",
    "    global x\n",
    "    global y\n",
    "    global sum\n",
    "    global i \n",
    "    sum = x + y\n",
    "    x = y\n",
    "    y = sum\n",
    "    print(x,y)\n",
    "    i += 1\n",
    "    if lim > i:\n",
    "        return fib(10)\n",
    "    else:\n",
    "        return fib\n",
    "\n",
    "fib(10)"
